# Register properties
IceGrid.Registry.Client.Endpoints=tcp -p 5061
IceGrid.Registry.Server.Endpoints=tcp
IceGrid.Registry.Internal.Endpoints=tcp
IceGrid.Registry.AdminPermissionsVerifier=IceGrid/NullPermissionsVerifier
IceGrid.Registry.Data=./data/registry


# Node properties 
IceGrid.Node.Endpoints=tcp
IceGrid.Node.Name=NodeName
IceGrid.Node.Data=./data/node
# 当Registry和Node一起启动的时候 需要用到CollocateRegistry=1
IceGrid.Node.CollocateRegistry=1
Ice.Default.Locator=IceGrid/Locator:tcp -p 5061
